{
  "name": "three-field-date",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://resmio.github.io/three-field-date",
  "dependencies": {
    "@resmio/rollico": "^2.0.0",
    "glamor": "^2.20.24",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "enzyme": "^2.8.2",
    "gh-pages": "^0.12.0",
    "react-scripts": "0.9.5",
    "react-test-renderer": "^15.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "yarn run build && gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.js"
